<%-include("partials/header.ejs") %> 

    <main id="main">
      <!-- ======= Hero Slider Section ======= -->
      <section id="hero-slider" class="hero-slider">
        <div class="container-md" data-aos="fade-in">
          <div class="row">
            <div class="col-12">
              <div class="swiper sliderFeaturedPosts">
                <div class="swiper-wrapper">

                  <% for(let i=0;i<sliderPosts.length;i++) {%> 

                    <div class="swiper-slide">
                      <a
                        href="/posts/<%=sliderPosts[i].id %> "
                        class="img-bg d-flex align-items-end"
                        style="
                          background-image: url('<%=sliderPosts[i].yoast_head_json.og_image[0].url %>');
                        "
                      >
                        <div class="img-bg-inner">
                          <h2>
                            <%- sliderPosts[i].title.rendered %> 
                          </h2>
                          <p>
                            <%- sliderPosts[i].excerpt.rendered %>
                          </p>
                        </div>
                      </a>
                    </div>

                    <% } %> 
                  

                </div>
                <div class="custom-swiper-button-next">
                  <span class="bi-chevron-right"></span>
                </div>
                <div class="custom-swiper-button-prev">
                  <span class="bi-chevron-left"></span>
                </div>

                <div class="swiper-pagination"></div>
              </div>
            </div>
          </div>
        </div>
      </section>
      <!-- End Hero Slider Section -->

      <!-- ======= Post Grid Section ======= -->
      <section id="posts" class="posts">
        <div class="container" data-aos="fade-up">
          <div
            class="section-header d-flex justify-content-between align-items-center mb-5"
          >
            <h2>Latest From Freelancing</h2>
            <div><a href="/categories/freelancing" class="more">See All Posts</a></div>
          </div>
          <div class="row g-5">
            <div class="col-lg-4">
