                <a href="/posts/<%=latestFreelancingPosts[0].id %>"
                  ><img
                    src="<%=latestFreelancingPosts[0].yoast_head_json.og_image[0].url %> "
                    alt=""
                    class="img-fluid"
                /></a>
                <div class="post-meta">
                  <span class="mx-1">&bullet;</span> <span><%=latestFreelancingPosts[0].date %> </span>
                </div>
                <h2>
                  <a href="/posts/<%=latestFreelancingPosts[0].id %>"
                    ><%-latestFreelancingPosts[0].title.rendered %> </a
                  >
                </h2>
                <p class="mb-4 d-block">
                  <%-latestFreelancingPosts[0].excerpt.rendered %>
                </p>

                <div class="d-flex align-items-center author">
                  <div class="name">
                    <h3 class="m-0 p-0"><%=latestFreelancingPosts[0].yoast_head_json.author %></h3>
                  </div>
                </div>
              </div>
            </div>

            <div class="col-lg-8">
