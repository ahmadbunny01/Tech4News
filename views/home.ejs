                  </div>
                </div>

                <!-- Trending Section -->
                <div class="col-lg-4">
                  <div class="trending">
                    <h3>More Posts</h3>
                    <ul class="trending-post">

                      <% for(let i =7;i<latestFreelancingPosts.length;i++) {%> 
                        <li>
                          <a href="/posts/<%=latestFreelancingPosts[i].id %>">
                            <h3>
                              <%-latestFreelancingPosts[i].title.rendered %>
                            </h3>
                            <span class="author"><%=latestFreelancingPosts[i].yoast_head_json.author %></span>
                          </a>
